	<e:Scroller id="sc_portrait" width="100%" y="877">
		<e:Group id="gp_portrait">
			<e:List id="list_invite" itemRendererSkinName="InvitePortraitItemRenderSkin" scaleX="1" scaleY="1" itemRenderer="InvitePortraitItemRender">
				<e:layout>
				<e:HorizontalLayout gap="45" paddingLeft="20" paddingRight="20"/>
				</e:layout>
			</e:List>
			<e:layout>
				<e:HorizontalLayout horizontalAlign="left"/>
			</e:layout>
		</e:Group>
	</e:Scroller>
