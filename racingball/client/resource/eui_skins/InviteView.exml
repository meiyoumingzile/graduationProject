						<e:BasicLayout/>
					</e:layout>
				</e:Group>
			</e:Skin>
		</e:skinName>
	</e:Button>
	<e:Button id="btn_play" label="" y="627" scaleX="1" scaleY="1" horizontalCenter="0" icon="invite-icon-friend.png" visible="false">
		<e:skinName>
			<e:Skin states="up,down,disabled">
			<e:Image width="100%" height="100%" source="skin-btn-play.png" source.down="skin-btn-play-down.png"/>
