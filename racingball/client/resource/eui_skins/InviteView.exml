			<e:Skin states="up,down,disabled">
				<e:Image width="100%" height="100%"  source="invite-btn.png" source.down="invite-btn-down.png"/>
				<e:Group anchorOffsetY="0" horizontalCenter="0" height="95" y="0" y.down="3">
					<e:Image id="iconDisplay" verticalCenter="0" y="74" scaleX="1" scaleY="1"  source="invite-icon-friend.png"/>
