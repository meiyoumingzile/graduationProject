	<e:Image y="271.5" source="invite-skin-bg.png" horizontalCenter="0" />
	<e:Scroller id="sc_skins" width="100%" y="320" horizontalCenter="0" height="253">
		<e:Group id="gp_skin">
			<e:layout>
				<e:HorizontalLayout gap="0" paddingRight="0" verticalAlign="middle" />
			</e:layout>
		</e:Group>
	</e:Scroller>
